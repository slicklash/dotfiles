snippet     perm
abbr        List: Permutation
options     head
	function perm(xs) {
		if (xs.length < 2) return [xs]
		const [head, ...tail] = xs
		const result = []
		for (const p of perm(tail)) {
			for (let i = 0; i < xs.length; i++) {
				const copy = [...p]
				copy.splice(i, 0, head)
				result.push(copy)
			}
		}
		return result
	}
