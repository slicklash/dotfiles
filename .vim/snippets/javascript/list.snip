snippet     comb
abbr        List: Combinations
options     head
	const combinations = (xs, k) => {
	  if (k == 0) return[[]]
	  if (k == xs.length) return [xs]
	  const [head, ...tail] = xs
	  return combinations(tail, k - 1)
	    .map(c => [head, ...c])
	    .concat(combinations(tail, k))
	}
