snippet ptt
abbr prototype
alias proto
options head
  ${1:#:class_name}.prototype.${2:#:method_name} = function(${3:#:first_argument}) {
    ${0:TARGET}
  };


snippet html5
alias doc5
options head
    <!DOCTYPE html>
    <html>
        <head>
            <meta http-equiv="content-type" content="text/html; charset=${1:utf-8}">
            <title>${2}</title>
        </head>
        <body>
            ${3}
        </body>
    </html>
